{
  "source": {
    "includePattern": ".+\\.ts(doc|x)?$",
    "excludePattern": ".+\\.(test|spec).ts"
  },
  "sourceType": "module",
  "recurseDepth": 2,
  "plugins": ["node_modules/better-docs/typescript"],
  "tags": { "allowUnknownTags": true },
  "opts": { "template": "node_modules/better-docs" }
}
